# AWS IVS Configuration
# Copy this file to .env.local and fill in your actual values

# IVS Stage ARN (for interactive streaming)
NEXT_PUBLIC_IVS_STAGE_ARN=arn:aws:ivs:us-east-1:123456789012:stage/your-stage-id

# Participant Token for IVS Stage (required for stage access)
NEXT_PUBLIC_IVS_PARTICIPANT_TOKEN=your-participant-token-here

# Chat Room ARN (for chat functionality)
NEXT_PUBLIC_IVS_CHAT_ROOM_ARN=arn:aws:ivs-chat:us-east-1:123456789012:room/your-chat-room-id 